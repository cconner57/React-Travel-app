{
	"$id": "http://example.com/example.json",
	"type": "object",
	"additionalProperties": false,
	"properties": {
        "category": {
			"$id": "/properties/category",
			"type": "string",
			"title": "The Category Schema ",
			"default": "General",
			"examples": ["General"]
		},
		"title": {
			"$id": "/properties/title",
			"type": "string",
			"title": "The Title Schema ",
			"default": "",
			"minLength": 1,
			"maxLength": 55,
			"examples": ["Post Title"]
		},
		"message": {
			"$id": "/properties/message",
			"type": "string",
			"title": "The Message Schema ",
			"default": "",
			"minLength": 1,
			"maxLength": 255,
			"examples": ["Message Content"]
		}
	}
}
