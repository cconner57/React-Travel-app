{
	"$id": "http://example.com/example.json",
	"type": "object",
	"additionalProperties": false,
	"properties": {
        "email": {
			"$id": "/properties/email",
			"type": "string",
			"minLength": 6,
			"maxLength": 60,
			"title": "The Email Schema ",
			"default": "",
			"examples": ["johnsmith@gmail.com"]
		},
		"password": {
			"$id": "/properties/password",
			"type": "string",
			"title": "The Password Schema ",
			"default": "",
			"minLength": 5,
			"maxLength": 55,
			"examples": ["foo123"]
		},
		"first_name": {
			"$id": "/properties/first_name",
			"type": "string",
			"title": "The First_name Schema ",
			"default": "",
			"minLength": 3,
			"maxLength": 55,
			"examples": ["John"]
		},
		"last_name": {
			"$id": "/properties/last_name",
			"type": "string",
			"title": "The Last_name Schema ",
			"default": "",
			"minLength": 1,
			"maxLength": 55,
			"examples": ["Smith"]
		}
	},
	"required": ["username", "password", "email"]
}
